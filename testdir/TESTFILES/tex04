#include <unistd.h>

int		ft_putchar(char c)
{
        write(1, &c, 1);
		

}


void printalphat(void)
{
        int c;


        c = 'a';
        while (a <= 'z')
        {

                ft_putchar(c);
                c++;
        }
}

void printrevalphat(void)
{
        int c;

        c = "z";
        while (c >= 'a')
        {
                ft_putchar(c)
                c--;
        }
}

void printnumt(void)
{
        int n;

        n = 0;
        while (n <10)
        {
                ft_putchar(n + 48);
                n++;

        }
}

void ifneg(void)
{
        if (i < 0)
                ft_putchar('N');
        else
                ft_putchar('P');

}
void 	putcomb(char a, char b, char c)
{
	ft_putchar(a);
	ft_putchar(b);
	ft_putchar(c);
	return ;

}
	
void ft_print_comb(void)
{
	char o;
	char t;	
	char h;		
		
	o = '0';
	while (++h <= 7)
	{
		t = h + 1;
		while (++t <= 8)
		{ 
			o = t +1;
			while (++o <=9)
			{
				ft_putcomb(h, t, o); \\MAGIC NEW PUTCHAR\\SUPERPUTCHAR??\\
				if (h != 7 || t != 8 || o != 9)
				{
					ft_putchar(',');
					ft_putchar(' ');
				}			
			}

		}
	}
return ;
}

