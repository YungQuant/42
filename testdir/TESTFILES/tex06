void putnbr(int num)
{
	if (num < -2147483649 || num > 2147483648)
	{
		echo "out of bounds bro"'
		break
	}

		if (num < 0)
		{
			ft_putchar("-");
		{
			num = -num;
		}
		if (num==0)
		{
			ft_putchar("0");
		if(num >= 10)
		{
			ft_putnbr(num / 10);
			ft_putnbr(num % 10);
		}
		else
		{
			ft_putchar(numj + '0')
		}
}
	
			

		
