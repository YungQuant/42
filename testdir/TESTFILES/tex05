void diffcomb(void)
{
        char a;
        char b

        a = '0'
        while (++a <= 9)
        {
                b = a++;
                while (++b <= 9)
                {
					
                                ft_putchar(a, b); \\MAGIC NEW PUTCHAR\\SUPERPUTCHAR??\\
                                if (a != 9 && b != 9)
                                {
                                        ft_putchar(',');
                                        ft_putchar(' ');
                                }
                        }

                }
        }
}




